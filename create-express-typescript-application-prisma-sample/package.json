{
  "name": "create-express-typescript-application-sample",
  "version": "0.0.1",
  "description": "create-express-typescript-application-sample",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && cd build && node ./build/index.js",
    "start:dev": "nodemon -w src -w prisma -e ts,prisma --exec ts-node ./src/index.ts",
    "dev": "cross-env NODE_ENV=development npm run prisma:update && concurrently \"npm run tsoa:generate_\" \"npm run prisma:sync_\" \"npm run start:dev\"",
    "prebuild": "npm run tsoa:generate && cross-env NODE_ENV=production npm run prisma:update && npm run prisma:sync",
    "build": "tsc",
    "tsoa:generate_": "nodemon -w src -w prisma -e ts generate-tsoa.js",
    "tsoa:generate": "node generate-tsoa.js",
    "prisma:sync_": "nodemon -w prisma -e prisma,ts sync-prisma.js",
    "prisma:sync": "node sync-prisma.js",
    "prisma:update": "node update-prisma.js",
    "prisma:studio": "npx prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.17",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "child_process": "^1.0.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.15.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "helmet": "^4.3.1",
    "morgan": "^1.10.0",
    "prisma": "^2.16.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.5.1"
  }
}
